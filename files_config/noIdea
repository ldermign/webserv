server {
	server_name exemple.fr etpuipouet;
	listen 8080;
	# error_page 404 ./html/index.html;
	client_max_body_size 50;
	
	location ./html/
	{
		get_methods GET POST DELETE;
		root html;
		index index.html;
		
		return 401 error_page;
		autoindex on;
		cgi .php index.html;
		download html;
	}
}

